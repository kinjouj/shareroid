{
  "name": "shareroid",
  "version": "0.1",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "img/icon_18.png"
  },
  "background": {
    "scripts": [
      "lib/js/jquery.js",
      "lib/js/underscore.js",
      "lib/js/chrome_ex_oauth.js",
      "lib/js/chrome_ex_oauthsimple.js",
      "const.js",
      "share.js",
      "shareroid.js",
      "background.js"
    ],
    "persistent": false
  },
  "options_page": "options.html",
  "permissions": ["contextMenus", "alarms", "tabs", "notifications", "https://*/*"],
  "web_accessible_resources": ["img/icon_48.png"],
  "content_security_policy": "script-src 'self' 'unsafe-eval';object-src 'self'"
}

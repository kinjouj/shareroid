{
  "name": "shareroid",
  "version": "0.0.2",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "img/icon_18.png"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "permissions": [
    "identity",
    "alarms",
    "notifications",
    "tabs",
    "https://shareroid.appspot.com/"
  ]
}
